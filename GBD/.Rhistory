mapping = aes(x = location, y = val, fill = measure, alpha = 0.8),
stat = "identity", width = 0.3) +
facet_wrap(vars(sex))+
labs(x = "", y = "DALYs", fill = "") +
ggthemes::theme_base() +
theme(
